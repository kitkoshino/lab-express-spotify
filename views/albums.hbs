<html>
<head>
  <link rel="stylesheet" href="/styles/style.css">  
</head>
  <body>
    <div class="albuns-container">
    {{#each artistAlbuns}}
    <div class="album-card-bigger">
      <img src="{{images.[0].url}}" alt="" style="width: 400px; height: 400px">
      <div class="album-name-container">
        <li><h4>{{name}}</h4></li>
        <button class="album-button"><a href="/album-tracks/{{id}}">View Tracks</a></button>
      </div>
    </div>
    {{/each}}

    </div>
  </body>
</html>